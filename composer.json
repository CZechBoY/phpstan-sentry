{
	"name": "phpstan/phpstan-sentry",
	"description": "Consistence Sentry class reflection extension for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "0.11-dev"
		}
	},
	"require": {
		"php": "~7.1",
		"phpstan/phpstan": "^0.11",
		"consistence-php/consistence": "dev-master"
	},
	"require-dev": {
		"consistence/coding-standard": "^3.0.1",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"phing/phing": "^2.13.0",
		"phpstan/phpstan-phpunit": "^0.11",
		"phpstan/phpstan-strict-rules": "^0.11",
		"phpunit/phpunit": "^7.0",
		"slevomat/coding-standard": "^4.5.2"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:slevomat/consistence.git"
		}
	]
}
