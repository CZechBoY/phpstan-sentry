{
	"name": "phpstan/phpstan-sentry",
	"description": "Consistence Sentry class reflection extension for PHPStan",
	"license": ["MIT"],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "0.10-dev"
		}
	},
	"require": {
		"php": "~7.1",
		"phpstan/phpstan": "^0.10",
		"consistence-php/consistence": "dev-master"
	},
	"require-dev": {
		"consistence/coding-standard": "~0.13.0",
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"phing/phing": "^2.13.0",
		"phpstan/phpstan-phpunit": "^0.10",
		"phpstan/phpstan-strict-rules": "^0.10",
		"phpunit/phpunit": "^7.0",
		"slevomat/coding-standard": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"PHPStan\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": ["tests/"]
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:slevomat/consistence.git"
		}
	]
}
